<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Vendor Creation Request Sequence -->
        <record id="seq_vendor_creation_request" model="ir.sequence">
            <field name="name">Vendor Creation Request</field>
            <field name="code">vendor.creation.request</field>
            <field name="prefix">VCR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

