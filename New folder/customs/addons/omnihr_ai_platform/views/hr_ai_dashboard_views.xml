<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        
        <!-- AI Dashboard Action -->
        <record id="action_hr_ai_dashboard" model="ir.actions.client">
            <field name="name">AI Dashboard</field>
            <field name="tag">hr_ai_dashboard</field>
        </record>
        
        <!-- Add remaining actions for other models -->
        <record id="action_hr_sentiment_analysis" model="ir.actions.act_window">
            <field name="name">Sentiment Analysis</field>
            <field name="res_model">hr.sentiment.analysis</field>
            <field name="view_mode">list,form</field>
        </record>
        
        <record id="action_hr_ai_task" model="ir.actions.act_window">
            <field name="name">AI Tasks</field>
            <field name="res_model">hr.ai.task</field>
            <field name="view_mode">list,form</field>
        </record>
        
        <record id="action_hr_ai_insights" model="ir.actions.act_window">
            <field name="name">AI Insights</field>
            <field name="res_model">hr.ai.insights</field>
            <field name="view_mode">list,form</field>
        </record>
        
        <record id="action_hr_predictive_models" model="ir.actions.act_window">
            <field name="name">Predictive Models</field>
            <field name="res_model">hr.predictive.models</field>
            <field name="view_mode">list,form</field>
        </record>
        
    </data>
</odoo> 