<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sequence for DPR Code -->
    <record id="seq_architect_dpr" model="ir.sequence">
        <field name="name">DPR Sequence</field>
        <field name="code">architect.dpr</field> <!-- This must match your next_by_code() -->
        <field name="prefix">DPR/</field>
        <field name="padding">4</field> <!-- Will generate DPR/0001, DPR/0002, etc. -->
        <field name="number_increment">1</field>
    </record>
</odoo>