<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequence for Manufacturing Estimations -->
        <record id="seq_mrp_estimation" model="ir.sequence">
            <field name="name">Manufacturing Estimation</field>
            <field name="code">mrp.estimation</field>
            <field name="prefix">EST/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence for Manufacturing Costing -->
        <record id="seq_mrp_costing" model="ir.sequence">
            <field name="name">Manufacturing Costing</field>
            <field name="code">mrp.costing</field>
            <field name="prefix">COST/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Estimation Version Sequence -->
        <record id="seq_mrp_estimation_version" model="ir.sequence">
            <field name="name">Estimation Version</field>
            <field name="code">mrp.estimation.version</field>
            <field name="prefix">V</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
