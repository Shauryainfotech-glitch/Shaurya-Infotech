<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- AI Analysis Configuration Data -->
        <record id="ai_analysis_config" model="ir.config_parameter">
            <field name="key">location_tracker.ai_enabled</field>
            <field name="value">True</field>
        </record>

        <record id="ai_confidence_threshold" model="ir.config_parameter">
            <field name="key">location_tracker.ai_confidence_threshold</field>
            <field name="value">0.7</field>
        </record>

        <record id="anomaly_detection_enabled" model="ir.config_parameter">
            <field name="key">location_tracker.anomaly_detection_enabled</field>
            <field name="value">True</field>
        </record>

        <record id="max_speed_threshold" model="ir.config_parameter">
            <field name="key">location_tracker.max_speed_threshold</field>
            <field name="value">120</field>
        </record>

        <record id="gps_accuracy_threshold" model="ir.config_parameter">
            <field name="key">location_tracker.gps_accuracy_threshold</field>
            <field name="value">100</field>
        </record>

    </data>
</odoo>
